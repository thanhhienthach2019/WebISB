
@{
    Layout = null;
}

<!DOCTYPE html>
<link href="~/Content/Error/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<link href="~/Content/Error/css/style.css" rel="stylesheet" />
<script src="~/Content/Error/js/bootstrap.min.js"></script>
<script src="~/Content/Error/js/jquery-1.11.1.min.js"></script>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>PageNotfound</title>
</head>
<body>
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="error-template">
                    <h1>
                        Oops!
                    </h1>
                    <h2>
                        Unknown HTTP Request Method
                    </h2>
                    <div class="error-details">
                        Sorry, standard web clients will not issue HTTP requests with unknown methods!
                    </div>
                    <div class="error-actions">
                        <a href="@Url.Action("Index", "TrangChu")" class="btn btn-primary btn-lg"><i class="fa fa-home"></i> Take Me Home</a>
                        <a href="#" class="btn btn-default btn-lg"><span class="glyphicon glyphicon-envelope"></span> Contact Support </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
